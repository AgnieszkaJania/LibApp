@model LibApp.Models.Book

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Book Name: @Model.AuthorName</li>
    <li>Book Name: @Model.Genre</li>
    <li>Book Name: @Model.DateAdded</li>
    <li>Book Name: @Model.ReleaseDate</li>
    <li>Book Name: @Model.NumberInStock</li>
   
</ul>